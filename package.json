{
  "name": "dolbyio-spatial-audio-demo-web",
  "version": "1.0.0",
  "description": "",
  "main": "desktop.js",
  "scripts": {
    "start": "node server.js",
    "desktop": "nds desktop.js",
    "electron:mac": "npx electron-builder --macos --x64 -c.mac.identity=null",
    "electron:win": "npx electron-builder --windows --x64"
  },
  "build": {
    "appId": "io.dolby.spatial-audio",
    "productName": "Dolby.io Spatial Audio",
    "electronVersion": "9.4.4",
    "electronDist": "./node_modules/@dolbyio/native-desktop-sdk/dist",
    "files": [
      "src/images/*.png",
      "src/scripts/*.js",
      "src/**.*",
      "desktop.js",
      "preload.js",
      "package.json"
    ],
    "directories": {
      "buildResources": ".",
      "output": "output"
    },
    "pkg": {
      "allowCurrentUserHome": false,
      "allowAnywhere": false
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "src/icon.png"
    }
  },
  "keywords": [
    "spatial-audio",
    "dolby.io"
  ],
  "author": "Fabien Lavocat",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/dolbyio-samples/dolbyio-spatial-audio-demo-web/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dolbyio-samples/dolbyio-spatial-audio-demo-web.git"
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "commander": "^8.3.0",
    "dotenv": "^10.0.0",
    "electron-builder": "^22.13.1",
    "express": "^4.17.1"
  },
  "dependencies": {
    "@dolbyio/dolbyio-rest-apis-client": "^0.1.0-beta.2",
    "@dolbyio/native-desktop-sdk": "^1.0.0"
  }
}
